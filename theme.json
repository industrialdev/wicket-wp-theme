    "custom": {
      "border-radius": {
        "0": "0rem",
        "025": "0.125rem",
        "050": "0.25rem",
        "100": "0.5rem",
        "150": "0.75rem",
        "200": "1rem",
        "250": "1.25rem",
        "300": "1.5rem",
        "400": "2rem",
        "500": "2.5rem",
        "600": "3rem"
      },
      "box-shadow": {
        "4": "0px 4px 8px 0px rgba(35, 42, 49, 0.20)",
        "8": "0px 8px 12px 0px rgba(0, 0, 0, 0.25)",
        "12": "0px 12px 12px 0px rgba(0, 0, 0, 0.25)",
        "focus": "0px 0px 1px 2px #0997FF"
      },
      "layout": {
        "content": "1320px",
        "wide": "1320px",
        "sidebar": "322px"
      },
      "letter-spacing": {
        "none": "normal",
        "tight": "-.01em",
        "loose": ".05em",
        "looser": ".1em"
      },
      "line-height": {
        "tiny": 1.1,
        "small": 1.2,
        "medium": 1.4,
        "normal": 1.6
      }
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "spacingSizes": [
        {
          "size": "0.125rem",
          "slug": "25",
          "name": "25 (2px)"
        },
        {
          "size": "0.25rem",
          "slug": "50",
          "name": "50 (4px)"
        },
        {
          "size": "0.375rem",
          "slug": "75",
          "name": "75 (6px)"
        },
        {
          "size": "0.5rem",
          "slug": "100",
          "name": "100 (8px)"
        },
        {
          "size": "0.75rem",
          "slug": "150",
          "name": "150 (12px)"
        },
        {
          "size": "1rem",
          "slug": "200",
          "name": "200 (16px)"
        },
        {
          "size": "1.25rem",
          "slug": "250",
          "name": "250 (20px)"
        },
        {
          "size": "1.5rem",
