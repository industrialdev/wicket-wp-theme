          "name": "600 (48px)"
        },
        {
          "size": "4rem",
          "slug": "800",
          "name": "800 (64px)"
        },
        {
          "size": "5rem",
          "slug": "1000",
          "name": "1000 (80px)"
        }
      ]
    },
    "typography": {
      "dropCap": false,
      "lineHeight": false,
      "customFontSize": false,
      "letterSpacing": false,
      "fontFamilies": [
        {
          "fontFamily": "Montserrat,sans-serif",
          "name": "System Sans Serif",
          "slug": "system-sans-serif"
        },
        {
          "fontFamily": "-apple-system-ui-serif,ui-serif,Noto Serif,Iowan Old Style,Apple Garamond,Baskerville,Times New Roman,Droid Serif,Times,Source Serif Pro,serif,Apple Color Emoji,Segoe UI Emoji,Segoe UI Symbol",
          "name": "System Serif",
          "slug": "system-serif"
        }
      ],
      "fontSizes": [
        {
          "name": "Heading 3xl",
          "size": "clamp(2.5rem, 3vw + 1rem, 3.25rem)",
          "slug": "heading-3xl"
        },
        {
          "name": "Heading 2xl",
          "size": "clamp(2.25rem, 3vw + 1rem, 2.75rem)",
          "slug": "heading-2xl"
        },
        {
          "name": "Heading xl",
          "size": "clamp(2rem, 2vw + 1rem, 2.25rem)",
          "slug": "heading-xl"
        },
        {
          "name": "Heading lg",
          "size": "clamp(1.75rem, 2vw + 1rem, 2rem)",
          "slug": "heading-lg"
        },
        {
          "name": "Heading md",
          "size": "clamp(1.5rem, 2vw + 1rem, 1.75rem)",
          "slug": "heading-md"
        },
        {
          "name": "Heading sm",
          "size": "clamp(1.25rem, 1vw + 1rem, 1.5rem)",
          "slug": "heading-sm"
        },
        {
          "name": "Heading xs",
          "size": "clamp(1.125rem, 1vw + 1rem, 1.25rem)",
          "slug": "heading-xs"
        },
        {
          "name": "Body lg",
          "size": "20px",
          "slug": "body-lg"
        },
        {
          "name": "Body md",
          "size": "16px",
          "slug": "body-md"
        },
        {
          "name": "Body sm",
          "size": "14px",
          "slug": "body-sm"
        }
      ]
    },
    "layout": {
      "contentSize": "var(--wp--custom--layout--content)",
      "wideSize": "var(--wp--custom--layout--wide)"
    },
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": true
        }
      },
      "core/group": {
        "color": {
          "background": true
        },
        "border": {
          "radius": true
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--foreground)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--custom--color--link)"
        }
      },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--dark)",
          "text": "var(--wp--preset--color--light)"
        },
        "border": {
          "width": "2px",
          "radius": "var(--wp--custom--border-radius--050)",
          "style": "solid",
          "color": "var(--wp--preset--color--dark)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--150)",
            "right": "var(--wp--preset--spacing--200)",
            "bottom": "var(--wp--preset--spacing--150)",
            "left": "var(--wp--preset--spacing--200)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--body-md)",
          "fontWeight": "700",
          "lineHeight": "150%"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--system-serif)",
          "fontWeight": "900",
          "lineHeight": "var(--wp--custom--line-height--small)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--heading-3-xl)",
          "lineHeight": "clamp(3rem, 3vw + 1rem, 3.75rem)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--heading-2-xl)",
          "lineHeight": "clamp(2.5rem, 3vw + 1rem, 3.25rem)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--heading-xl)",
          "lineHeight": "clamp(2rem, 2vw + 1rem, 2.25rem)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "lineHeight": "clamp(2.25rem, 3vw + 1rem, 2.5rem)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "lineHeight": "clamp(2rem, 2vw + 1rem, 2.25rem)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "clamp(1.625rem, 1.5vw + 1rem, 1.75rem)"
        }
      }
    },
    "spacing": {
      "blockGap": "0.75rem"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--system-sans-serif)",
      "lineHeight": "var(--wp--custom--line-height--normal)",
      "fontSize": "var(--wp--preset--font-size--medium)"
    },
    "blocks": {
      "core/quote": {
        "border": {
          "width": "0"
        },
        "spacing": {
          "padding": {
            "left": "0"
          }
        },
        "typography": {
          "fontSize": "24px",
          "fontWeight": "400",
          "lineHeight": "var(--wp--custom--line-height--medium)",
          "fontStyle": "italic"
        }
      },
      "wicket/dynamically-related-content": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--500)",
            "bottom": "var(--wp--preset--spacing--500)"
          }
        }
      }
    }
  }
}
